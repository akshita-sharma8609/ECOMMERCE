
<script>
    let form = document.getElementById('admin_forgot_password_form');
    form.addEventListener('submit', function (e) {
        e.preventDefault();
    
        let formData = new FormData(this);
    
            fetch("/forgotpassword", {
            method: 'POST',
            body: formData 
        }).then(res => res.json())
            .then(function (res) {
                console.log(res)
                if (res.msg == "changepassword") {
                    console.log("ye chl rha h")
                    window.location.href = "http://localhost:3000/adminchangepassword";
                   
                } else if (res.msg == "pswrdmessage") {
                    window.location.href = "http://localhost:3000/pswrdmessage"
    
                } 
            }).catch(function (err) {
                console.log(err);
            })
    }) 
    </script>